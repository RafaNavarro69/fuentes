from suds.client import Client
import os
import time

os.environ["NLS_LANG"] = "American_America.UTF8" 

url_consulta='http://localhost:8080/WSVado/WSVado?wsdl'

consulta = Client(url_consulta)
print(consulta)
print(time.strftime("%H:%M:%S"))

respuesta=consulta.service.vado('5379')
print(respuesta)

respuesta=consulta.service.actualizaVado('5379','646464646')
print(respuesta)

